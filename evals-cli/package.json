{
  "name": "@mcpjam/cli",
  "version": "1.1.2",
  "type": "module",
  "description": "MCPJam CLI for programmatic MCP testing and evals",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "repository": {
    "type": "git",
