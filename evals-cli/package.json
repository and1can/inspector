    "directory": "cli"
  },
  "main": "dist/index.js",
  "bin": {
    "mcpjam": "bin/mcpjam.js"
  },
  "files": [
    "bin",
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
