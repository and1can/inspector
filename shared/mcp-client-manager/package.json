{
  "name": "@mcpjam/mcp-client-manager",
  "version": "0.1.0",
  "type": "module",
  "description": "MCP client manager helpers built on top of @modelcontextprotocol/sdk",
  "license": "Apache-2.0",
  "author": "MCPJam (https://mcpjam.com)",
  "homepage": "https://mcpjam.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/MCPJam/inspector.git",
    "directory": "shared/mcp-client-manager"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup index.ts --clean --format esm,cjs --dts --sourcemap --out-dir dist --external @modelcontextprotocol/sdk",
    "dev": "tsup index.ts --watch --format esm,cjs --dts --sourcemap --out-dir dist --external @modelcontextprotocol/sdk",
    "test": "npm run build && tsc --project ./tsconfig.test.json && node ./dist/tests/tests/test.js",
    "prepublishOnly": "npm run build"
  },
  "peerDependencies": {
    "@modelcontextprotocol/sdk": "^1.17.3"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.6.3"
  }
}
